-- My Furry Version Of Ambush And Rush
-- Custom Rush
local RushMoving = game.Workspace:WaitForChild("RushMoving")

if RushMoving = then

local RushMoving = game.Workspace:FindFirstChild("RushMoving")

if RushMoving = then

game.Workspace["RushMoving"].RushNew.Attachment.ParticleEmitter.Texture = LoadCustomAsset("https://github.com/TheendisnearDonotfear/Others/blob/main/mauzymice.png?raw=true")
game.Workspace["RushMoving"].RushNew.PlaySound.SoundId = "rbxassetid://4702564143"
game.Workspace["RushMoving"].RushNew.PlaySound.PlaybackSpeed = 1
game.Workspace["RushMoving"].RushNew.PlaySound.Volume = 2
game.Workspace["RushMoving"].RushNew.Footsteps.SoundId = "rbxassetid://4702564143"
game.Workspace["RushMoving"].RushNew.Footsteps.PlaybackSpeed = 1
game.Workspace["RushMoving"].RushNew.Footsteps.Volume = 3

game.Workspace["RushMoving"].RushNew.Attachment.BlackTrail.Color = ColorSequence.new({
            ColorSequenceKeypoint.new(0, Color3.new(0, 0, 0)),
            ColorSequenceKeypoint.new(0.305882, Color3.new(0, 0, 0)),
            ColorSequenceKeypoint.new(0.305882, Color3.new(0, 0, 0)),
            ColorSequenceKeypoint.new(1, Color3.new(0, 0, 0))
        })

end

-- Fury Ambush
local AmbushMoving = game.Workspace:WaitForChild("AmbushMoving")

if AmbushMoving = then

local AmbushMoving = game.Workspace:FindFirstChild("AmbushMoving")

if AmbushMoving = then

game.Workspace["AmbushMoving"].AmbushNew.Attachment.ParticleEmitter.Texture = LoadCustomAsset("https://github.com/TheendisnearDonotfear/FearmodeDoors/blob/main/Ambush_Furry.png?raw=true")
game.Workspace["AmbushMoving"].AmbushNew.PlaySound.SoundId = "rbxassetid://4702564143"
game.Workspace["AmbushMoving"].AmbushNew.PlaySound.PlaybackSpeed = 1
game.Workspace["AmbushMoving"].AmbushNew.PlaySound.Volume = 2
game.Workspace["AmbushMoving"].AmbushNew.Footsteps.SoundId = "rbxassetid://4702564143"
game.Workspace["AmbushMoving"].AmbushNew.Footsteps.PlaybackSpeed = 1
game.Workspace["AmbushMoving"].AmbushNew.Footsteps.Volume = 3
        })

end

